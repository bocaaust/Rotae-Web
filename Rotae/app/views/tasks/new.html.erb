<h1>New Task</h1>

<%= form_for :task, url: tasks_path do |f| %>
  <p>
    <%= f.label :task_name %><br>
    <%= f.text_field :task_name %>
  </p>
 
  <p>
    <%= f.label :task_time %><br>
    <%= f.text_area :task_time %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', tasks_path %>